{% extends 'base.html.twig' %}

{% block title %}Ajouter un pays{% endblock %}

{% block body %}
    <div class="block-page">
        <h1 class="block-name">Ajouter un Pays</h1>
        {{ form_start(paysForm, {'attr': {'novalidate': 'novalidate'}}) }}
        {{ form_widget(paysForm) }}
        <div class="button">
            <button class="btn-add">Ajouter</button>
        </div>
        {{ form_end(paysForm) }}
    </div>
{% endblock %}
