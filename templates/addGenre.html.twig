{% extends 'base.html.twig' %}

{% block title %}Ajouter un genre{% endblock %}

{% block body %}
    <div class="block-page">
        <h1 class="block-name">Ajouter un Genre</h1>
        {{ form_start(genreForm, {'attr': {'novalidate': 'novalidate'}}) }}
