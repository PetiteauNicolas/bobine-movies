{% extends 'base.html.twig' %}

{% block title %}Ajouter un acteur{% endblock %}

{% block body %}
    <div class="block-page">
        <h1 class="block-name">Ajouter un Acteur</h1>
        {{ form_start(acteurForm, {'attr': {'novalidate': 'novalidate'}}) }}
        {{ form_widget(acteurForm) }}
        <div class="button">
            <button class="btn-add">Ajouter</button>
        </div>
        {{ form_end(acteurForm) }}
    </div>
{% endblock %}
